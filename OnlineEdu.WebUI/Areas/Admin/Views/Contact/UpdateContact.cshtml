@model UpdateContactDto
@{
    ViewData["Title"] = "Update Contact";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form method="post">
	<input type="hidden" asp-for="ContactId" />
	<label>Map Url</label>
	<input type="text" asp-for="MapUrl" class="form-control" />
	<br />
	<label>Address</label>
	<input type="text" asp-for="Address" class="form-control" />
	<br />
	<label>Phone Number</label>
	<input type="text" asp-for="PhoneNumber" class="form-control" />
	<br />
	<label>Email</label>
	<input type="text" asp-for="Email" class="form-control" />
	<br />
	<button class="btn btn-outline-primary">Save</button>
	<button class="btn btn-outline-danger" asp-action="Index">Cancel</button>
</form>
